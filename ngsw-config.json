{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [{
    "name": "",
    "urls": [
      "/movie",
      "/discover",
      "/search"
    ],
    "cacheConfig": {
      "maxSize": 500,
      "maxAge": "1h",
      "timeout": "10s",
      "strategy": "freshness"
    }
  }]
}
